{
  "name": "MGoBlog Dark Mode",
  "description": "Override MGoBlog styles for a dark mode user experience.",
  "version": "1.2",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "css": ["mgoblog-dark-mode.css"],
      "matches": ["https://mgoblog.com/*"],
      "run_at": "document_start"
    }
  ]
}
